{
  "name": "ts-framework-app",
  "description": "A TSFramework Example Web Application",
  "version": "0.0.3",
  "scripts": {
    "prestart": "tsc -p app",
    "start": "node build/app.js",
    "pretest": "node build/app.js & echo $! > ci.pid & sleep 5",
    "test": "mocha build-test --recursive",
    "posttest": "kill -9 `cat ci.pid`"
  },
  "dependencies": {
    "typescript": "1.8.10",
    "tsframework-full": "^0.0.1",
    "huject": "^1.3.4"
  },
  "devDependencies": {
    "browserify": "^13.1.0",
    "chai": "^3.5.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-browser": "^3.0.2",
    "gulp-cli": "github:gulpjs/gulp-cli",
    "gulp-concat": "^2.6.0",
    "gulp-less": "^3.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-typescript": "^2.13.6",
    "mocha": "^2.4.5",
    "nodemon": "^1.10.2",
    "request": "~2.71.0",
    "should": "~8.3.0",
    "vinyl-transform": "^1.0.0"
  }
}
